{
    "name": "envaysoft/fleetcart",
    "description": "The FleetCart E-Commerce System.",
    "type": "project",
    "authors": [
        {
            "name": "Mehedi Hassan",
            "email": "MehediDracula@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "astrotomic/laravel-translatable": "^11.9",
        "cache/filesystem-adapter": "^1.0",
        "cache/predis-adapter": "^1.0",
        "cartalyst/sentinel": "^5.1",
        "darryldecode/cart": "^4.2",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "florianv/laravel-swap": "^2.3",
        "florianv/swap": "4.1",
        "jackiedo/dotenv-editor": "^1.0",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.1",
        "laravel/legacy-factories": "^1.0",
        "laravel/scout": "^9.4",
        "laravel/tinker": "^2.0",
        "league/omnipay": "^3.2",
        "maatwebsite/laravel-sidebar": "~2.1",
        "mcamara/laravel-localization": "^1.7",
        "mehedi/stylist": "dev-master",
        "mews/captcha": "^3.2",
        "mexitek/phpcolors": "dev-master",
        "myonlinestore/omnipay-klarna-checkout": "^3.1",
        "nwidart/laravel-modules": "^8.0",
        "nyholm/psr7": "^1.5",
        "php-http/curl-client": "^2.2",
        "php-http/message": "^1.13",
        "spatie/schema-org": "^3.4",
        "stripe/stripe-php": "^8.8",
        "swayok/alternative-laravel-cache": "^5.4",
        "symfony/intl": "^5.0",
        "tightenco/ziggy": "^1.4",
        "typicms/nestablecollection": "1.1.21",
        "wikimedia/composer-merge-plugin": "^2.0",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.0",
        "beyondcode/laravel-dump-server": "^1.0",
        "beyondcode/laravel-query-detector": "^1.0",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "nunomaduro/collision": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "FleetCart\\": "app/",
            "Modules\\": "Modules/",
            "Themes\\": "Themes/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": "dist",
		"platform-check": false,
        "platform": {
            "php": "7.3.0"
        },
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "nwidart/laravel-modules"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "merge-plugin": {
            "require": [
                "Modules/*/composer.json",
                "Themes/*/composer.json"
            ],
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        }
    }
}
